{
    "targetAppName": "target-app-1",
    "configDataForCreation": {
        "apiVersion": "apps/v1",
        "kind": "Deployment",
        "metadata": {
           "name": "target-app-1"
        },
        "spec": {
           "replicas": 1,
           "revisionHistoryLimit": 10,
           "selector": {
              "matchLabels": {
                 "app": "nginx"
              }
           },
           "template": {
              "metadata": {
                 "labels": {
                    "app": "nginx"
                 }
              },
              "spec": {
                 "containers": [
                    {
                       "name": "nginx",
                       "image": "nginx:1.14",
                       "ports": [
                          {
                             "containerPort": 80
                          }
                       ]
                    }
                 ]
              }
           }
        }
     },
    "configDataForDeletion": {
        "gracePeriodSeconds": 0,
        "orphanDependents": false
    }
}